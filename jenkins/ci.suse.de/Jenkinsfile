pipeline {
    agent any
    stages {
        stage('Setting up') {
            agent {
                label "openstack-trackupstream"
            }
            steps {
                echo "Checking out package from OBS"
                sh "jenkins/ci.suse.de/update-package.sh"
                echo "Submitting to staging project"
                echo "Waiting for package to build"
                echo "Setting up test nodes"
                echo "Setting up repositories"
            }
        }
        stage('Installing') {
            steps {
                echo "Installing"
            }
        }
        stage('Testing') {
            steps {
                echo "Installing"
            }
        }
        stage('Results') {
            steps {
                echo "Installing"
            }
        }
    }
}
