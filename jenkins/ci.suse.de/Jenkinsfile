pipeline {
    agent any
    stages {
        stage('Preparation') {
            steps {
                sh "python3 checkout-pull-requests.py ${github_repo} ${pull_request_id}"
            }
        }
        stage('Build') {
            steps {
                echo "Building"
            }
        }
        stage('Results') {
            steps {
                echo "Done"
            }
        }
    }
}
